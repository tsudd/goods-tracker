COLUMN TABLE BROCHURE (
    ID INTEGER NOT NULL COMMENT 'Brochure ID',
    VENDOR_ID INTEGER NOT NULL COMMENT 'Issued vendor',
    BROCHURE_FILE BLOB MEMORY THRESHOLD 1000 NOT NULL COMMENT 'File',
    DATE_ADDED TIMESTAMP COMMENT 'When brochure was added',
    BROCHURE_NAME VARCHAR(45) NOT NULL COMMENT 'Brochure name',
    PRIMARY KEY (ID)
) COMMENT 'Brochure' WITH ASSOCIATIONS (
    JOIN "VENDOR" AS "VENDORS" ON "VENDORS"."ID" = "VENDOR_ID"
)