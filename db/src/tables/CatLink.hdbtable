COLUMN TABLE "CATLINK" (
    "ITEMID" INT COMMENT 'Item ID',
    "CATEGORYID" INT COMMENT 'Category ID',
    PRIMARY KEY ("ITEMID", "CATEGORYID")
) COMMENT 'Category Link' WITH ASSOCIATIONS (
    JOIN "CATEGORY" AS "CATEGORIES" ON "CATEGORIES"."ID" = "CATEGORYID",
    JOIN "ITEM" AS "ITEMS" ON "ITEMS"."ID" = "ITEMID"
)