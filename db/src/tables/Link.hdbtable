COLUMN TABLE "LINK" (
    "ITEMID" INTEGER NOT NULL COMMENT 'Item ID',
    "GOODID" INTEGER NOT NULL COMMENT 'Good ID',
    PRIMARY KEY("ITEMID", "GOODID")
) COMMENT 'Goods links' WITH ASSOCIATIONS (
    JOIN "ITEM" AS "ITEMS" ON "ITEMS"."ID" = "ITEMID",
    JOIN "GOOD" AS "GOODS" ON "GOODS"."ID" = "GOODID"
)