COLUMN TABLE "GOOD" (
    "ID" INTEGER NOT NULL COMMENT 'Good ID',
    "NAME1" VARCHAR(100) COMMENT 'Good Name 1',
    "NAME2" VARCHAR(100) COMMENT 'Good Name 2',
    "NAME3" VARCHAR(100) COMMENT 'Good Name 3',
    "IMAGE_LINK" VARCHAR(300) COMMENT 'Image link',
    "LAST_UPDATE" TIMESTAMP COMMENT 'Last update date',
    PRIMARY KEY ("ID")
) COMMENT 'Good'