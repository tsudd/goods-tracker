COLUMN TABLE "ITEMRECORD" (
    "STREAMID" INT COMMENT 'Items Stream',
    "ITEMID" INT COMMENT 'Item',
    "PRICE" DECIMAL(10, 2) COMMENT 'Price',
    "CUTPRICE" DECIMAL(10, 2) COMMENT 'Price with discount',
    "ONDISCOUNT" BOOLEAN COMMENT 'Does have discount'
) COMMENT 'Item record' WITH ASSOCIATIONS (
    JOIN "STREAM" AS "STREAMS" ON "STREAMS"."ID" = "STREAMID",
    JOIN "ITEM" AS "ITEMS" ON "ITEMS"."ID" = "ITEMID"
)