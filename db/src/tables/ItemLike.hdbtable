COLUMN TABLE "ITEMLIKE" (
    "ITEMID" INTEGER NOT NULL COMMENT 'Item ID',
    "USERID" VARCHAR(50) NOT NULL COMMENT 'User GUID',
    "DATE_ADDED" TIMESTAMP COMMENT 'When item was liked',
    PRIMARY KEY("ITEMID", "USERID")
) COMMENT 'Item likes' WITH ASSOCIATIONS (
    JOIN "ITEM" AS "ITEMS" ON "ITEMS"."ID" = "ITEMID",
    JOIN USER AS USERS ON USERS."ID" = "USERID"
)