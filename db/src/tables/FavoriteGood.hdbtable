COLUMN TABLE "FAVORITE_GOOD" (
    "GOOD_ID" INTEGER NOT NULL COMMENT 'Good ID',
    "USER_ID" VARCHAR(50) NOT NULL COMMENT 'User GUID',
    "DATE_ADDED" TIMESTAMP COMMENT 'When item was liked',
    PRIMARY KEY("GOOD_ID", "USER_ID")
) COMMENT 'Item likes' WITH ASSOCIATIONS (
    JOIN "GOOD" AS "GOODS" ON "GOODS"."ID" = "GOOD_ID",
    JOIN USER AS USERS ON USERS."ID" = "USER_ID"
)